{
  "name": "svelte-meta-tags",
  "version": "2.6.4",
  "description": "Svelte Meta Tags is a plugin that makes managing your SEO easier in Svelte projects.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package",
    "prepare": "husky install",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . --cache && eslint .",
    "format": "prettier --plugin-search-dir . --write . --cache",
    "test": "playwright test"
  },
  "dependencies": {
    "schema-dts": "^1.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.7",
    "@sveltejs/package": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "svelte": "^3.44.0"
  }
}
