{
  "name": "svelte-meta-tags",
  "version": "1.0.2",
  "description": "Svelte Meta Tags is a plug in that makes managing your SEO easier in Svelte projects.",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "package": "svelte-kit package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "pnpm cy:run",
    "prepare": "husky install",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "pnpm package && pnpm publish package"
  },
  "dependencies": {
    "schema-dts": "^0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@sveltejs/kit": "^v1.0.0-next.151",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-svelte3": "^3.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.7.4",
    "svelte2tsx": "^0.4.5",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "svelte": "^3.39.0"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts,svelte,md,html,css,json}": "pnpm format",
    "*.{js,ts,svelte}": [
      "pnpm lint",
      "pnpm check"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "ci",
          "chore",
          "docs",
          "feat",
          "fix",
          "refactor",
          "revert",
          "style",
          "test",
          "BREAKING CHANGE"
        ]
      ]
    }
  },
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  }
}
