{
  "name": "svelte-meta-tags",
  "version": "2.6.1",
  "description": "Svelte Meta Tags is a plugin that makes managing your SEO easier in Svelte projects.",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "svelte",
    "svelteKit",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit package",
    "prepare": "svelte-kit sync && husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --cache --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --cache --plugin-search-dir=. .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "pnpm cy:run",
    "release": "pnpm build && pnpm package && semantic-release"
  },
  "dependencies": {
    "schema-dts": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@sveltejs/adapter-auto": "1.0.0-next.63",
    "@sveltejs/kit": "1.0.0-next.392",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "cypress": "10.3.1",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-svelte3": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "semantic-release": "19.0.3",
    "svelte": "3.49.0",
    "svelte-check": "2.8.0",
    "svelte-preprocess": "4.10.7",
    "svelte2tsx": "0.5.13",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "3.0.4"
  },
  "peerDependencies": {
    "svelte": "^3.39.0"
  }
}
