{
  "name": "svelte-meta-tags",
  "version": "0.0.2",
  "description": "A plugin that facilitates the creation of meta tags for SEO in Svelte.",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "svelte": "./src/index.js",
  "scripts": {
    "develop": "rollup -cw",
    "format": "prettier --write --plugin-search-dir=. .",
    "build": "rollup -c",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "svelte",
    "metadata",
    "seo",
    "meta-tags",
    "open-graph",
    "svelte-meta-tags"
  ],
  "files": [
    "src",
    "dist",
    "@types",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oekazuma/svelte-meta-tags"
  },
  "bugs": {
    "url": "https://github.com/oekazuma/svelte-meta-tags/issues"
  },
  "dependencies": {
    "schema-dts": "^0.8.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "prettier": "^2.3.2",
    "rollup": "^2.53.1",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.38.3"
  }
}
